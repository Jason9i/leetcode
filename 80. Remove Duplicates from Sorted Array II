class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        len_n = len(nums)
        if len_n <= 2: 
            return len_n

        i = 2
        while i < len(nums):
            if nums[i-2] == nums[i]:
                nums.pop(i)
            else:
                i+=1
        return len_n        
