class Solution:
    def removeDuplicates(self, nums: List(int)) -> int:
        left, right = 0, 1

        while right < len(nums):
            if nums[left] == nums[right]:
                right += 1
            else: 
                nums[left+1] = nums[right]
                right+=1
                left+=1
               
        return left+1         






class Solution:
    def removeDuplicates(self, nums: List(int)) -> int:
        i = 1

        while i < len(nums):
            if nums[i-1] == nums[i]:
                nums.pop(i)
            else:
                i+=1
        return len(nums)            