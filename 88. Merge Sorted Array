class Solution:
    def merge(self, nums1: List[int], nums2: List[int], m: int, n: int ) -> None:
        if n == 0:
            return
        len_m=len(nums1)
        end_index = len_m -1

        while n> 0 and m > 0:
            if nums2[n-1] >= nums1[m-1]:
                nums1[end_index] = nums2[n-1]
                n -= 1
            else:
                nums1[end_index] = nums1[m-1]
                m -= 1
            end_index -= 1    
        while n> 0:
            nums1[end_index] = nums2[n-1]
            n-=1
            end_index -=1



class Solution:
    def merge(self, nums1: List[int], m: int, nums2: List[int], n: int ) -> None:
        a, b, end = m-1, n-1, m+n-1

        while b>=0:
            if a>= 0 and nums1[a] > nums2[b]:
                nums1[end] = nums1[a]
                a -= 1
            else:
                nums1[end] = nums2[b]
                b -=1

            end -=1








